<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:minWidth="25px"
	android:minHeight="25px">
	<LinearLayout
		android:orientation="horizontal"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/linearLayout1"
		android:background="@android:color/background_light"
		android:paddingTop="7.5pt"
		android:paddingBottom="5pt">
		<LinearLayout
			android:orientation="horizontal"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="175.5dp"
			android:layout_height="match_parent"
			android:id="@+id/linearLayout2" >
			<TextView
				android:text="1"
				android:layout_width="21.5dp"
				android:layout_height="wrap_content"
				android:id="@+id/textView_lecture_id"
				android:layout_marginLeft="5pt"
				android:layout_marginRight="5pt"
				android:textStyle="bold|italic"
				android:layout_marginTop="6.25pt" />
			<TextView
				android:text="Lecture Name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/txtv_lecture_name"
				android:layout_marginLeft="5pt"
				android:textStyle="bold|italic"
				android:textSize="20px"
				android:layout_marginTop="7.5pt"/>
		</LinearLayout>
		<LinearLayout
			android:orientation="horizontal"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:id="@+id/linearLayout3"
			android:layout_marginLeft="25pt">
			<ImageButton
				android:src="@drawable/openl"
				android:layout_height="57.5dp"
				android:id="@+id/btn_open_lec"
				android:maxWidth="0.0dp"
				android:layout_width="69.0dp"
				android:background="@android:color/background_light" />
			<ImageButton
				android:src="@android:drawable/ic_delete"
				android:layout_width="86.5dp"
				android:layout_height="57.0dp"
				android:id="@+id/btn_del_lec"
				android:layout_marginLeft="0.0dp"
				android:layout_marginRight="0.0dp"
				android:background="@android:color/background_light"
				android:layout_marginBottom="0.0dp" />
		</LinearLayout>
	</LinearLayout>

</LinearLayout>